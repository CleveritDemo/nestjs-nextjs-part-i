{
  "name": "nestjs-app",
  "version": "1.0.0",
  "description": "NestJS application with one entity called products",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "prestart": "npm run build",
    "build": "tsc",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:run": "npm run typeorm schema:drop && npm run typeorm schema:sync && npm run typeorm migration:run",
    "typeorm:generate": "npm run typeorm migration:generate -- -n",
    "typeorm:revert": "npm run typeorm migration:revert",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "postgres"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "class-validator": "^0.14.1",
    "faker": "^6.6.6",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.4"
  }
}
